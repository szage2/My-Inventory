<resources>
    <!-- These are the options displayed in the sale drop-down Spinner -->
    <string-array name="array_sale_options">
        <item>@string/no_sale</item>
        <item>@string/small_sale</item>
        <item>@string/medium_sale</item>
        <item>@string/huge_sale</item>
    </string-array>
    <!-- These are the options displayed in the status drop-down Spinner -->
    <string-array name="array_status_options">
        <item>@string/status_not_shipped</item>
        <item>@string/status_shipped</item>
        <item>@string/status_in_transit</item>
        <item>@string/status_delivered</item>
    </string-array>
    <!-- These are the options displayed in the delivery drop-down Spinner -->
    <string-array name="array_delivery_options">
        <item>@string/usual_delivery</item>
        <item>@string/expedite_delivery</item>
    </string-array>
</resources>